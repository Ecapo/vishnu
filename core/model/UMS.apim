    <services name="listUsers" description="lists VISHNU users" adminOnly="true" resultCodes="NO_ADMIN UNKNOWN_USERID SESSIONKEY_EXPIRED SESSIONKEY_NOT_FOUND INCORRECT_DATE_OPTION DB_ERROR UMS_SERVER_NOT_AVAILABLE">
