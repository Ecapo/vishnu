    <services name="deleteMachine" description="removes a machine from VISHNU" adminOnly="true" resultCodes="VISHNU_OK NO_ADMIN UNKNOWN_MACHINE SESSIONKEY_NOT_FOUND SESSIONKEY_EXPIRED DB_ERROR UMS_SERVER_NOT_AVAILABLE">
