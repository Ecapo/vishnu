#include <iostream>
//#include <stdlib>

#include "MapperRegistry.hh"
#include "Mapper_impl.hh"

int main(int argc, char** argv){
  string name = string("toto");
  int i;
  string command, key;
  if (argc>1)
    name = string(argv[1]);

  MapperRegistry* mr = new MapperRegistry();
  
  for (i=0;i<10;i++){
    name += string(i%4);
    if (!(i%2))
      key = string(command%3);
    Mapper_impl mapper(mr, name);
    mapper.registerMapper();
    if (i%2)
      mapper.getCommand(key, command);
  }

}
