            <row>
              <entry><para>sessionKey</para></entry>
              <entry><para>string</para></entry>
              <entry><para>The key of the session of the user that submits the command</para></entry>
              <entry><para>IN</para></entry>
              <entry><para>yes</para></entry>
            </row>
            <row>
              <entry><para>machineId</para></entry>
              <entry><para>string</para></entry>
              <entry><para>The id of the machine to update</para></entry>
              <entry><para>IN</para></entry>
              <entry><para>yes</para></entry>
            </row>
            <row>
              <entry><para>CompoMachine</para></entry>
