					<xsd:enumeration value="copy"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="PidType">
				<xsd:annotation>
					<xsd:documentation>Sha1 unique reference</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[^\s]{40,40}"/>
				</xsd:restriction>
			</xsd:simpleType>
